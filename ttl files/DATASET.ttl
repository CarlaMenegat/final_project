                #BRACELET 
<item/Bracelet> a crm:E22_Man-Made_Object ;
    crm:P2_has_type "ArchaeologicalArtifact" ;
    crm:P45_consists_of [
        a crm:E57_Material ;
        rdfs:label "Gold"
    ] ;
    crm:P4_has_time-span <time-span/PtolemaicPeriod> ;
    dct:provenance <place/Egypt> ;
    crm:P50_has_current_keeper </agent/TheBritishMuseum> ;
    crm:P32_used_general_technique "Ciselure" ;
    crm:P32_used_general_technique "Incision" ;
    crm:P32_used_general_technique "Twisting" ;
    crm:P108_has_produced </agent/AncientEgyptians> ;
    crm:P32_acquired_title_through <event/bequeathal> .

</agent/TheBritishMuseum> a crm:E39_Actor ;
    crm:P48_has_preferred_identifier "TheBritishMuseum" .

<event/bequeathal> a crm:E10_Transfer_of_Custody ;
    crm:P30_transferred_custody_of <agent/TheBritishMuseum> ;
    crm:P31_has_modified <item/bracelet> ;
    crm:P32_acquired_title_through "Sir Augustus Wollaston Franks" ;
    dct:date "1897" .


